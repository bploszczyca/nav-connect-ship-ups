{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "al",
            "request": "launch",
            "name": "Your own server",
            "server": "http://BC19-NC",
            "serverInstance": "BC",
            "authentication": "Windows",
            "startupObjectId": 22,
            "startupObjectType": "Page",
            "breakOnError": true,
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default",            
            "schemaUpdateMode": "ForceSync"            
        },
        {
            "name": "My attach to local server: WebClient",
            "type": "al",
            "request": "attach",
            "server": "http://BC19-NC",
            "serverInstance": "BC",
            "authentication": "Windows",
            "breakOnError": true,            
            "breakOnRecordWrite": false,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "breakOnNext": "WebClient"
        },
        {
            "name": "My attach to local server: WebServiceClient",
            "type": "al",
            "request": "attach",
            "server": "http://BC19-NC",
            "serverInstance": "BC",
            "authentication": "Windows",
            "breakOnError": true,
            "breakOnRecordWrite": false,
            "enableSqlInformationDebugger": true,
            "enableLongRunningSqlStatements": true,
            "longRunningSqlStatementsThreshold": 500,
            "numberOfSqlStatements": 10,
            "breakOnNext": "WebServiceClient"
        }
    ]
}